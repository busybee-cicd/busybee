  "devDependencies": {
    "@types/async": "^2.0.46",
    "@types/body-parser": "^1.16.8",
    "@types/glob": "^5.0.34",
    "@types/lodash": "^4.14.104",
    "@types/lodash.clonedeep": "^4.5.3",
    "@types/node": "^10.0.5",
    "@types/portscanner": "^2.1.0",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^5.1.2",
    "ava": "^0.23.0",
    "busybee-html-reporter": "2.0.5",
