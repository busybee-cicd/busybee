{
  "envResources": {
    "hosts": [
      {
        "name": "localhost",
        "capacity": 100
      }
    ]
  },
  "testSuites": [
    {
      "id": "My Test Suite",
      "type": "REST",
      "skip": false,
      "protocol": "http",
      "ports": [8180],
      "root": "/v1",
      "defaultRequestOpts": {
        "headers": {
          "my-header": "myHeaderValue"
        },
        "json": true
      },
      "env": {
        "parallel": true,
        "resourceCost": 50,
        "startScript": "envStart.sh",
        "stopScript": "envStop.sh",
        "healthcheck": {
          "type": "REST",
          "retries": 30,
          "request": {
            "endpoint": "/healthcheck",
            "query": {
              "myQueryParam": "myQueryValue"
            },
            "body": {
              "myBodyKey": "myBodyValue"
            },
            "timeout": 5000
          }
        }
      },
      "envInstances": [
        {
          "id": "My Test Env",
          "testSets": [
            {
              "id": "My Test Set"
            }
          ]
        }
      ],
      "mockServer": {
        "port": 3030,
        "root": "/my/mockserver/root/v1",
        "proxy": {
          "protocol": "http",
          "host": "localhost",
          "port": 3000
        },
        "injectedRequestOpts": {
          "headers": {
            "my-header": "myHeaderValue"
          },
          "query": {
            "myQueryParam": "myQueryValue"
          },
          "body": {
            "myBodyKey": "myBodyValue"
          }
        }
      }
    }
  ]
}
